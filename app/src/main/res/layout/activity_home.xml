<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Home"
    tools:layout_editor_absoluteY="25dp">

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="0dp"
        android:layout_height="230dp"
        android:layout_marginBottom="92dp"
        android:layout_marginTop="92dp"
        app:layout_constraintBottom_toBottomOf="@+id/history_button"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/intro_button"
        app:srcCompat="@drawable/cover" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginBottom="34dp"
        android:layout_marginTop="70dp"
        app:layout_constraintBottom_toTopOf="@+id/history_button"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/info_button"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/logo_processed" />

    <ImageButton
        android:id="@+id/info_button"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginEnd="42dp"
        android:layout_marginLeft="29dp"
        android:layout_marginRight="42dp"
        android:layout_marginStart="29dp"
        android:background="@drawable/info"
        app:layout_constraintBottom_toBottomOf="@+id/history_button"
        app:layout_constraintEnd_toStartOf="@+id/history_button"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/history_button"
        app:srcCompat="@android:color/transparent" />

    <ImageButton
        android:id="@+id/history_button"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginBottom="31dp"
        android:layout_marginEnd="16dp"
        android:layout_marginRight="16dp"
        android:background="@drawable/history"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/info_button"
        app:layout_constraintTop_toBottomOf="@+id/intro_button"
        app:srcCompat="@android:color/transparent" />

    <ImageButton
        android:id="@+id/intro_button"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginBottom="169dp"
        android:layout_marginTop="10dp"
        android:background="@drawable/intro"
        app:layout_constraintBottom_toTopOf="@+id/history_button"
        app:layout_constraintEnd_toEndOf="@+id/history_button"
        app:layout_constraintStart_toStartOf="@+id/history_button"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@android:color/transparent" />


</android.support.constraint.ConstraintLayout>